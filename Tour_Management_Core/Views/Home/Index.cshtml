@model List<Tour_Management_Core.Models.Tour>

<!DOCTYPE html>
<html>
<head>
    <title>Tour Management</title>
</head>
<body>
    <h1>Tours</h1>
    <ul>
        @foreach(var tour in Model)
        {
            <li>@tour.Name - @tour.Description - @tour.Location - $@tour.Price</li>
        }
    </ul>
</body>
</html>
